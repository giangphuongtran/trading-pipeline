# Database configuration
POSTGRES_HOST=localhost
POSTGRES_PORT_LOCAL=your_local_port_here # E.g. 5432
POSTGRES_PORT_DOCKER=5432
POSTGRES_DB=your_db_name
POSTGRES_USER=your_user_name
POSTGRES_PASSWORD=your_db_password

# Polygon API
# Get your Polygon API key from https://polygon.io/
POLYGON_API_KEY=your_polygon_api_key

# Airflow Database
AIRFLOW_USERNAME=your_airflow_user_name
AIRFLOW_PASSWORD=your_airflow_password
AIRFLOW_EMAIL=your_airflow_email
AIRFLOW_DATABASE_URL=postgresql+psycopg2://your_user_name:your_password@postgres:5432/airflow
AIRFLOW_PORT=your_airflow_port # E.g. 8080
AIRFLOW_FERNET_KEY=your_fernet_key_here  # Will be auto-generated by setup script